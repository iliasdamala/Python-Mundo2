times =('Palmeiras','Flamengo','Atletico mineiro','Gremio',
        'Sao paulo','Santos','Interational','Fluminense','Bragantino',
        'Atletico Paranense','Corinthians','Fortaleza','Bahia',
        'Sport Recife','Ceara','Cuiaba','Juventude','Chapecoense',
        'Atletico goianiense','America mineiro')
print('=+=+='*10)
print(f'Lista de times do Brasileirao: {times}')
print('=+=+='*10)
print(f'Os 5 primeiros sao: {times[0:5]}')
print('=+=+='*10)
print(f'Os 4 ultimos times sao: {times[-4:]}')
print('=+=+='*10)
print(f'os times em ordem alfabetico: {sorted(times)}')
print('=+=+='*10)
print(f'O chapecoense esta na {times.index("Chapecoense")+1}Âª posicao')


