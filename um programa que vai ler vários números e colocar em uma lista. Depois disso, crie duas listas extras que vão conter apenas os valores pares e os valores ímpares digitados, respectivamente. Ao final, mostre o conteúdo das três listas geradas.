num = []
pares = []
impares = []
while True:
    num.append(int(input('Digite um valor: ')))
    resp = str(input('Quer continuar? [S/N]:'))
    if resp in 'Nn':
        break
for i,v in enumerate(num):
    if v % 2 == 0:
        pares.append(v)
    else:
        impares.append(v)
print('=-'*30)
print(f'Voce Digitou os numeros {num}')
print(f'Os numeros pares da lista sao {pares}')
print(f'Os numeros impares da lsita sao {impares}')



