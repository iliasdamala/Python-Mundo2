num = int(input('digite um numero:'))
tot=0
for n in range(1,num + 1):
    if n % 2 == 0:
        print('\033[33m', end=' ')
        tot += 1
    else :
        print('\033[31m', end=' ')
    print('{}'.format(n), end=' ')
print('\n\33[m0 numero {} foi divisivel {} vezes'.format(n,tot))
if tot==2:
    print('E por isso é PRIMO!')
else:
    print('E por isso Nao é PRIMO!')
